{
    "name": "tiny-ipinfo",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm-run-all clean server:build client:build",
        "client:start": "npx parcel src/ui/index.html --port 1234 --dist-dir dist/ui",
        "client:build": "npx parcel build src/ui/index.html --dist-dir dist/ui",
        "server:start": "nodemon dist/api/index.js",
        "server:build": "npx tsc",
        "dev": "IPINFO_ENV=dev npm-run-all server:build server:start",
        "prod": "IPINFO_ENV=prod npm-run-all server:build server:start",
        "migrations:up": "./node_modules/db-migrate/bin/db-migrate up --migrations-dir ./dist/api/migrations",
        "migrations:down": "./node_modules/db-migrate/bin/db-migrate down --migrations-dir ./dist/api/migrations",
        "migrations:reset": "./node_modules/db-migrate/bin/db-migrate reset --migrations-dir ./dist/api/migrations",
        "stress-test": "./node_modules/artillery/bin/run run artillery.yml --output stress-test.json",
        "stress-test:report": "./node_modules/artillery/bin/run report --output dist/ui/report.html stress-test.json",
        "cache:dataset": "node dist/api/bin/cacheDataset.js",
        "cache:users": "node dist/api/bin/cacheUsers.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "ajv": "^8.11.0",
        "artillery": "^2.0.0-23",
        "bootstrap": "^5.2.1",
        "combinatorial-generators": "^1.1.2",
        "csv-parse": "^5.3.0",
        "db-migrate": "^0.11.13",
        "db-migrate-sqlite3": "^0.4.0",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "ioredis": "^5.2.3",
        "ip-to-int": "^0.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.1",
        "sqlite3": "^5.1.1",
        "winston": "^3.8.2",
        "axios": "^0.27.2",
        "chart.js": "^3.9.1",
        "chartjs-adapter-date-fns": "^2.0.0",
        "date-fns": "^2.29.3",
        "react-bootstrap-icons": "^1.9.1",
        "react-chartjs-2": "^4.3.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.5.0",
        "@parcel/transformer-typescript-tsc": "^2.7.0",
        "@types/express": "^4.17.14",
        "@types/ioredis": "^4.28.10",
        "@types/nodemon": "^1.19.2",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/sqlite3": "^3.1.8",
        "buffer": "^6.0.3",
        "concurrently": "^7.4.0",
        "nodemon": "^2.0.20",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.7.0",
        "process": "^0.11.10",
        "rimraf": "^3.0.2",
        "@types/react-router-dom": "^5.3.3",
        "typescript": "^4.8.4"
    }
}
